function fuck($s){
	$arr=str_split($s);
	//print_r($arr);
	$arr1[0]=$arr[0];
    $arr1[1]=$arr[1];
    $ht=implode("",$arr1);
    $h=$ht+12;
    $ht=strval($h);
    
   // return $h;
     $arr2[4]=$arr[3];
    $arr2[5]=$arr[4];
    $m=implode("",$arr2);
     $arr3[7]=$arr[6];
    $arr3[8]=$arr[7];
    $s=implode("",$arr3);
    return $h.":".$m.":".$s;
	
}
function fuck1($s){
	$arr=str_split($s);
	//print_r($arr);
	$arr1[0]=$arr[0];
    $arr1[1]=$arr[1];
    $ht=implode("",$arr1);
    $h=$ht;
    $ht=strval($h);
    
   // return $h;
     $arr2[4]=$arr[3];
    $arr2[5]=$arr[4];
    $m=implode("",$arr2);
     $arr3[7]=$arr[6];
    $arr3[8]=$arr[7];
    $s=implode("",$arr3);
    return $h.":".$m.":".$s;
	
}


fscanf($handle, "%s",$s);
$result = timeConversion($s);
